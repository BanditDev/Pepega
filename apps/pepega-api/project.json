{
  "sourceRoot": "apps/pepega-api/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/node:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/pepega-api",
        "main": "apps/pepega-api/src/main.ts",
        "tsConfig": "apps/pepega-api/tsconfig.app.json",
        "assets": ["apps/pepega-api/src/assets"],
        "generatePackageJson": true
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false,
          "fileReplacements": [
            {
              "replace": "apps/pepega-api/src/environments/environment.ts",
              "with": "apps/pepega-api/src/environments/environment.prod.ts"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "@nrwl/node:node",
      "options": {
        "buildTarget": "pepega-api:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/pepega-api/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/pepega-api"],
      "options": {
        "jestConfig": "apps/pepega-api/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "generate": {
      "executor": "@nrwl/workspace:run-commands",
      "outputs": [],
      "options": {
        "command": "prisma generate --schema ./libs/pepega/prisma/schema.prisma"
      }
    },
    "dbpush": {
      "executor": "@nrwl/workspace:run-commands",
      "outputs": [],
      "options": {
        "command": "prisma db push --schema ./libs/pepega/prisma/schema.prisma"
      }
    },
    "studio": {
      "executor": "@nrwl/workspace:run-commands",
      "outputs": [],
      "options": {
        "command": "prisma studio --schema ./libs/pepega/prisma/schema.prisma"
      }
    }
  },
  "tags": []
}
