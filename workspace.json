{
  "version": 2,
  "projects": {
    "dream": {
      "root": "apps/dream",
      "sourceRoot": "apps/dream",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": ["{options.outputPath}"],
          "defaultConfiguration": "production",
          "options": {
            "root": "apps/dream",
            "outputPath": "dist/apps/dream"
          },
          "configurations": {
            "production": {},
            "development": {}
          }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": {
            "buildTarget": "dream:build",
            "dev": true
          },
          "configurations": {
            "production": {
              "buildTarget": "dream:build:production",
              "dev": false
            },
            "development": {
              "buildTarget": "dream:build:development",
              "dev": true
            }
          },
          "defaultConfiguration": "development"
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": {
            "buildTarget": "dream:build:production"
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/dream"],
          "options": {
            "jestConfig": "apps/dream/jest.config.ts",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/dream/**/*.{ts,tsx,js,jsx}"]
          }
        }
      },
      "tags": []
    },
